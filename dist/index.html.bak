                <!-- https://startbootstrap.com/solution/contact-forms-->
                <form id="contactForm" data-sb-form-api-token="API_TOKEN">
                    <div class="row align-items-stretch mb-5">
                        <div class="col-md-6">
                            <div class="form-group">
                                <!-- Ime i prezime -->
                                <input class="form-control" id="name" type="text" placeholder="Ime i prezime*" data-sb-validations="required" />
                                <div class="invalid-feedback" data-sb-feedback="name:required">Obavezno polje.</div>
                            </div>
                            <div class="form-group">
                                <!-- E-mail -->
                                <input class="form-control" id="email" type="email" placeholder="E-mail*" data-sb-validations="required,email" />
                                <div class="invalid-feedback" data-sb-feedback="email:required">Obavezno polje.</div>
                                <div class="invalid-feedback" data-sb-feedback="email:email">Neispravno uneto polje.</div>
                            </div>
                            <div class="form-group mb-md-0">
                                <!-- Broj telefona -->
                                <input class="form-control" id="phone" type="tel" placeholder="Broj telefona*" data-sb-validations="required" />
                                <div class="invalid-feedback" data-sb-feedback="phone:required">Obavezno polje.</div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group form-group-textarea mb-md-0">
                                <!-- Motivaciono pismo -->
                                <textarea class="form-control" id="message" placeholder="Zašto želiš da nam se priključiš?*" data-sb-validations="required"></textarea>
                                <div class="invalid-feedback" data-sb-feedback="message:required">Obavezno polje.</div>
                            </div>
                        </div>
                    </div>
                    <!-- Uspešna prijava -->
